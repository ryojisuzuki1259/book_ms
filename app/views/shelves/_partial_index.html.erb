<% shelves.each do |shelf| %>
  <%= shelf.name %>
  <%= form_with model:shelf, url:shelf_path(shelf), local:true do |f| %>
    <%= f.radio_button :is_active, :true %>
    <%= f.label :is_active, "有効" %>
    <%= f.radio_button :is_active, :false %>
    <%= f.label :is_active, "無効" %>
    <%= f.submit "更新する"%>
  <% end %>
<% end %>