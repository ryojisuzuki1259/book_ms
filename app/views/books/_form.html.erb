<div>
<%= form_with model:book, local:true do |f| %>
    <%= f.label :"書籍タイトル" %>
    <%= f.text_field :title %>
    <%= f.label :"ジャンル" %>
    <%= f.collection_select :genre_id, Genre.all, :id, :name %>
    <%= f.label :"発行年月日" %>
    <%= f.date_field :date_publication %>
    <%= f.label :"本棚" %>
    <%= f.collection_select :shelf_id, Shelf.all, :id, :name %>
    
    <div><%= f.submit "登録する"%></div>
<% end %>
</div>